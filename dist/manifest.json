{
  "manifest_version": 2,
  "name": "falabella-status",
  "version": "0.1.0",
  "description": "Falabella's Chrome extension to see configuration objects",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_title": "Falabella Status",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js",
      "popup.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "<all_urls>"
  ]
}